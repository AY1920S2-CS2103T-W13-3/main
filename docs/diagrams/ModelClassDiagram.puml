@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class Model
Class ProfileList
Class ProfileManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Profile {
Class Profile
Class DeadlineList
Class SemModHashMap
}


Package Deadline {
Class Deadline
}
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

ProfileManager .up.|> Model
Model .right.> ObservableList
ProfileManager *--> ProfileList
ProfileManager *-left-> UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

ProfileList *--> "1"Profile

Profile *--right> DeadlineList
Profile *--down> SemModHashMap
DeadlineList ..up|> ObservableList


DeadlineList *--> "*"Deadline

@enduml


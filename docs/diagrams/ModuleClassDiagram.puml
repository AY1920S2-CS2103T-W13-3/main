@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor STORAGE_COLOR
skinparam classBackgroundColor STORAGE_COLOR

Class ModuleList

ModuleList .down.> Module

package Module {
    Class Module
    Class ModuleCode
    Class Title
    Class PreReqs
    Class Preclusions
    Class ModularCredits
